<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.DepositoController"
            prefHeight="500.0" prefWidth="500.0">

    <VBox alignment="CENTER" spacing="10" layoutX="125" layoutY="80">
        <Label text="Depositar Dinero" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
        <TextField fx:id="cantidadField" promptText="Cantidad a depositar"/>
        <Button text="Depositar" onAction="#depositarDinero"/>
        <Button text="Volver" onAction="#volverAlDashboard"/>
    </VBox>

</AnchorPane>