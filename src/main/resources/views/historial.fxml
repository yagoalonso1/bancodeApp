<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.HistorialController"
            prefHeight="500.0" prefWidth="500.0">

    <VBox spacing="15" alignment="CENTER" layoutX="50" layoutY="50">
        <Label text="Historial de Operaciones" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>
        <ListView fx:id="historialList" prefHeight="300" prefWidth="400"/>
        <Button fx:id="volverButton" text="Volver al Dashboard" onAction="#volverAlDashboard"/>
    </VBox>

</AnchorPane>